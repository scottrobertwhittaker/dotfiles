[user]
    name = Scott Whittaker
[alias]
    ci = commit
    co = checkout
    br = branch
    fe = fetch --prune
    lg = log --pretty=format:'%C(yellow)%h %C(red)%d %Creset %s %C(cyan) %an' --graph
    st = status -sb
[color]
    branch = auto
    diff = auto
    status = auto
    ui = auto
[core]
    editor = vim
    filemode = false
[pager]
    status = true
